{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/((?!api|_next|favicon.ico|manifest.json|robots.txt|sitemap.xml|og-image.png|assets).*)",
      "destination": "/"
    }
  ]
}
